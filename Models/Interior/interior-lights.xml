
<PropertyList>

 <path>interior-lights.ac</path>
 
 <animation>
  <type>select</type>
  <object-name>intlight.1</object-name>
  <object-name>intlight.2</object-name>
  <condition>
   <greater-than>
    <property>/systems/electrical/outputs/low-voltage/interior-lights</property>
	<value>10</value>
   </greater-than>
  </condition>
 </animation>
 
  <animation>
    <type>light</type>
    <light-type>point</light-type>
    <object-name>intlight.1</object-name>
	<nopreview/>
    <position>
      <x> 2.2 </x>
      <y> -0.35 </y>
      <z> 1.4 </z>
    </position>
    <ambient>
      <r>  0.5 </r>
      <g>  0.2 </g>
      <b>  0.2 </b>
      <a>  1.0 </a>
    </ambient>
    <diffuse>
      <r>  0.7 </r>
      <g>  0.3 </g>
      <b>  0.3 </b>
      <a>  0.9 </a>
    </diffuse>
    <specular>
      <r>  1.0 </r>
      <g>  0.4 </g>
      <b>  0.4 </b>
      <a>  1.0 </a>
    </specular>
     <dim-factor>
      <property>/instrumentation/illumination/red-floods</property>
      <factor>1</factor>
	  <offset>0</offset>
      <min>0</min>
      <max>1.0</max>
   </dim-factor>
   <attenuation>
     <c>1</c>
     <l>1</l>
     <q>1</q>
   </attenuation>
   <near-m>0.1</near-m>
   <far-m>10</far-m>
 </animation>
 
   <animation>
    <type>light</type>
    <light-type>point</light-type>
    <object-name>intlight.2</object-name>
	<nopreview/>
    <position>
      <x> 2.2 </x>
      <y> 0.35 </y>
      <z> 1.4 </z>
    </position>
    <ambient>
      <r>  0.5 </r>
      <g>  0.2 </g>
      <b>  0.2 </b>
      <a>  1.0 </a>
    </ambient>
    <diffuse>
      <r>  0.7 </r>
      <g>  0.3 </g>
      <b>  0.3 </b>
      <a>  0.9 </a>
    </diffuse>
    <specular>
      <r>  1.0 </r>
      <g>  0.4 </g>
      <b>  0.4 </b>
      <a>  1.0 </a>
    </specular>
     <dim-factor>
      <property>/instrumentation/illumination/red-floods</property>
      <factor>1</factor>
	  <offset>0</offset>
      <min>0</min>
      <max>1.0</max>
   </dim-factor>
   <attenuation>
     <c>1</c>
     <l>1</l>
     <q>1</q>
   </attenuation>
   <near-m>0.1</near-m>
   <far-m>10</far-m>
 </animation>

</PropertyList>