
<PropertyList>

 <path>headlights.ac</path>
 
   <animation>
    	<type>light</type>
		<light-type>spot</light-type>
		<object-name>beam.headlamp.left</object-name>
		<nopreview/>
		<direction><x>-1</x><y>-0.125</y><z>-0.75</z></direction>
		<position><x>-1</x><y>-0.7</y><z>0.589</z></position>
		<ambient><r>0.95</r><g>0.95</g><b>1.0</b><a>1</a></ambient>
		<diffuse><r>0.95</r><g>0.9</g><b>0.9</b><a>1</a></diffuse>
		<specular><r>0.95</r><g>0.9</g><b>0.9</b><a>1</a></specular>
		<attenuation><c>0.5</c><l>0.07</l><q>0</q></attenuation>
		<exponent>30.0</exponent>
		<cutoff>1000</cutoff>
		<near-m>0.5</near-m>
		<far-m>300</far-m>
		 <dim-factor>
         <property>/systems/headlamps/brightness-norm</property>
         <factor>1.0</factor>
		 <offset>0</offset>
         <min>0</min>
         <max>1.0</max>
		</dim-factor>
 </animation>
 
   <animation>
    	<type>light</type>
		<light-type>spot</light-type>
		<object-name>beam.headlamp.right</object-name>
		<nopreview/>
		<direction><x>-1</x><y>0.125</y><z>-0.75</z></direction>
		<position><x>-1</x><y>0.7</y><z>0.589</z></position>
		<ambient><r>0.95</r><g>0.95</g><b>1.0</b><a>1</a></ambient>
		<diffuse><r>0.95</r><g>0.9</g><b>0.9</b><a>1</a></diffuse>
		<specular><r>0.95</r><g>0.9</g><b>0.9</b><a>1</a></specular>
		<attenuation><c>0.5</c><l>0.07</l><q>0</q></attenuation>
		<exponent>30.0</exponent>
		<cutoff>1000</cutoff>
		<near-m>0.01</near-m>
		<far-m>300</far-m>
		 <dim-factor>
         <property>/systems/headlamps/brightness-norm</property>
         <factor>1.0</factor>
		 <offset>0</offset>
         <min>0</min>
         <max>1.0</max>
		</dim-factor>
 </animation>
  
 <animation>
  <type>material</type>
  <object-name>lens.headlamp.r</object-name>
  <object-name>lens.headlamp.l</object-name>
  <emission>
   <red>1</red>
   <green>1</green>
   <blue>0.85</blue>
   <factor-prop>/systems/headlamps/brightness-norm</factor-prop>
  </emission>
 </animation>

</PropertyList>