         <factor>1.0</factor>
		 <offset>-0.3</offset>
         <min>0</min>
         <max>1.0</max>
		</dim-factor>
 </animation>
 
 <animation>
    <type>light</type>
    <light-type>point</light-type>
    <object-name>beam.fog.left.nob</object-name>
	<nopreview/>
    <position>
      <x> -1.2 </x>
      <y> -0.6116 </y>
      <z> 0.5182 </z>
    </position>
    <ambient>
      <r>  0.5 </r>
      <g>  0.5 </g>
      <b>  0.45 </b>
      <a>  1.0 </a>
    </ambient>
    <diffuse>
      <r>  0.7 </r>
      <g>  0.7 </g>
      <b>  0.65 </b>
      <a>  0.9 </a>
    </diffuse>
    <specular>
      <r>  1.0 </r>
      <g>  1.0 </g>
      <b>  0.9 </b>
      <a>  1.0 </a>
    </specular>
     <dim-factor>
      <property>/systems/headlamps/foglights/brightness-norm</property>
